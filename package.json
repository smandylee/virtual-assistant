  "type": "module",
  "description": "Electron + LLM Assistant",
  "main": "apps/desktop/dist/main/index.js",
  "scripts": {
    "build:core": "tsc -p core/tsconfig.json",
    "build:desktop": "tsc -p apps/desktop/tsconfig.json && xcopy apps\\desktop\\src\\renderer apps\\desktop\\dist\\renderer /E /I /Y",
    "dev:core": "tsx core/src/api/server.ts",
    "dev:desktop": "NODE_ENV=development electron .",
    "dev:desktop:win": "set NODE_ENV=development && electron .",
    "dev:win": "concurrently \"npm:dev:core\" \"npm:dev:desktop:win\"",
    "dev": "concurrently \"npm:dev:core\" \"npm:dev:desktop\""
